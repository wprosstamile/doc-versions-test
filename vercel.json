{
  "version": 2,
  "routes": [
    {
      "src": "/sitemap.xml",
      "headers": {
        "Content-Type": "application/xml"
      },
      "dest": "/sitemap.xml"
    },
    {
      "src": "/v1/(.*)",
      "dest": "/v1/$1"
    },
    {
      "src": "/v2/(.*)",
      "dest": "/v2/$1"
    },
    {
      "src": "/current/(.*)",
      "dest": "/current/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 